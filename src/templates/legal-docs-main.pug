extends inc/page.pug
block template
  :php
    /*
    Template Name: Legal Docs Main Page
    */
block nav
  include ./inc/navbar-toggle.pug
  include ./inc/side-nav.pug
  include ./inc/header.pug
block content
  :php
    $breadcrumbs = [];
    array_push($breadcrumbs,array(
      "title"=>"Legal",
      "link"=>"/legal/"
    ));
    if( have_posts() ){
    while( have_posts() ) {
    the_post();
  section.top-section-interior
    .container
      .row
        .col-md-12
          include ./inc/breadcrumbs.pug
          h1
            :php
              the_title();
  section#legal-docs.container
    .row
      .col-md-3.legal-links
        include ./inc/legal-menu.pug
      .col-md-9.legal-box
        :php
          the_content();
          }}

        
