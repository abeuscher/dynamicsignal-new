.row.past-event-listing
  .col-md-12
    .inner
      p!=locals.start_month + " " + locals.start_day + ", " + locals.start_year
      h3!=locals.title
      .buttons.narrow
        if locals.webinar_recording_link
          a.button(href=locals.webinar_recording_link) Full Recording
        if locals.webinar_highlights_link
          a.button(href=locals.webinar_highlights_link) Highlights
